{
  "name": "video-camera-screen-recorder",
  "version": "1.0.0",
  "description": "HTML5 Video Camera & Screen Recorder. Record Video From Camera and Capture Screen using Javascript",
  "main": "./src/index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "contBuild": "watch \"npm run build:dev\" src",
    "devServer": "npm-run-all -p testServer contBuild",
    "start": "npm run devServer",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "prebuild": "rm -rf dist_old && mkdir dist_old && cp -a dist/. dist_old/",
    "test": "mocha --require @babel/register --require global-jsdom/register && jest",
    "jest": "jest",
    "testServer": "http-server --cors -p 8085 --silent",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/basil79/video-camera-screen-recorder.git"
  },
  "keywords": [
    "video-camera",
    "video-camera-recorder",
    "webcam",
    "webcam-recorder",
    "screen-capture",
    "video",
    "video-recorder",
    "video-camera-screen-recorder"
  ],
  "author": "Basil Goldman",
  "license": "Apache-2.0",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/register": "^7.16.5",
    "babel-eslint": "^10.1.0",
    "css-loader": "^5.0.0",
    "eslint": "^7.9.0",
    "eslint-plugin-import": "^2.22.0",
    "global-jsdom": "^8.4.0",
    "http-server": "^14.0.0",
    "jest": "^26.6.3",
    "jsdom": "^19.0.0",
    "mocha": "^9.2.2",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "should": "^13.2.3",
    "sinon": "^2.4.1",
    "string-replace-loader": "^2.3.0",
    "style-loader": "^3.3.1",
    "watch": "^0.13.0",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0"
  }
}
